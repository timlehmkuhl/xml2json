all(element)::= <<
{
  <element; separator=",\n">
}
>>

pair(name, content)::= <<
"<name>" : "<content; separator=",\n">"
>>

array(content)::= <<
[<content; separator=",">]
>>

pairarray(name, content)::= <<
"<name>" : ["<content; separator="\", \"">"]
>>

emptyarray(name)::= <<
"<name>" : []
>>

inlay(name, element)::= <<
"<name>" : {<element; separator=", ">}
>>

inlaypair(subname, content)::= <<
"<subname>":"<content; separator="\", \"">"
>>